**Swapping method**
vector<int> reverse(vector<int> a, int n){
	Int i = 0; 
	while(i <= n - i - 1){
		Int temp = a[i];
		a[i] = a[n - i - 1];
		A[n - i - 1] = temp;
		i++;
}
Return reverse;
}


***Recursion method***
Void reverse(int arr[], int start, int end){
	if(start > end){
		return;
  }
  Int temp = arr[start];
  Arr[start] = arr[end];
  Arr[end] = temp;
  reverse(arr, start+1, end-1);					
}

Int main(){
	Int arr[] = {1, 2, 3, 4, 5};
	Int n = arr.size();
	reverse(arr, 0, n-1); starting point 0 and ending point is n-1
	for(int i = 0; i < n; i++){
	  Cout << arr[i] << “ ”;
  }
}

